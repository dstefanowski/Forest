<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://kit.fontawesome.com/eab495fa99.js" crossorigin="anonymous"></script>
    <title>Oferta</title>
    <meta name="description" content="To jest opis meta description do nowej strony internetowej.">
    <!-- @include 'fonts.kit' -->
    <!-- @include 'styles.kit' -->
</head>

<body class="offer-page">
    <main>

        <section class="nav-offer-page">
            <!-- @include 'nav.kit' -->
        </section>

        <section class="offer" id="offer">
            <p class="section-caption">Wybierz jedną z naszych wspaniałych ofert!</p>
            <div class="wrapper">

                <div class="offer__container">
                    <div class="offer-box">
                        <div class="offer-card demo">
                            <div class="offer-card__title">Demo</div>
                            <div class="offer-card__price">$0</div>
                            <ul class="offer-card__options">
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option out"><i class="fa-solid fa-minus"></i>Opcja poza pakietem</li>
                                <li class="option out"><i class="fa-solid fa-minus"></i>Opcja poza pakietem</li>
                                <li class="option out"><i class="fa-solid fa-minus"></i>Opcja poza pakietem</li>
                            </ul>
                            <button class="offer-card__pick demo-btn">Wybierz</button>
                        </div>
                        <div class="offer-card standard">
                            <div class="offer-card__title">Standard</div>
                            <div class="offer-card__price">$99</div>
                            <ul class="offer-card__options">
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option out"><i class="fa-solid fa-minus"></i>Opcja poza pakietem</li>
                                <li class="option out"><i class="fa-solid fa-minus"></i>Opcja poza pakietem</li>
                            </ul>
                            <button class="offer-card__pick standard-btn default">Wybierz</button>
                        </div>
                        <div class="offer-card premium">
                            <div class="offer-card__title">Premium</div>
                            <div class="offer-card__price">$299</div>
                            <ul class="offer-card__options">
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                            </ul>
                            <button class="offer-card__pick premium-btn">Wybierz</button>
                        </div>
                        <div class="offer-card ultimate">
                            <div class="offer-card__title">Ultimate</div>
                            <div class="offer-card__price">$349</div>
                            <ul class="offer-card__options">
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option in"><i class="fa-solid fa-check"></i>Opcja w pakiecie</li>
                                <li class="option in special"><i class="fa-solid fa-plus"></i>Super dodatek</li>
                            </ul>
                            <button class="offer-card__pick ultimate-btn">Wybierz</button>
                        </div>
                    </div>
                </div>
            </div>
        </section>

    </main>

    <!-- @include 'footer.kit' -->

    <script src="dist/js/main.min.js"></script>
</body>

</html>